{
  "name": "bears-team-16",
  "version": "1.0.0",
  "description": "Mobile City, a shared experience game platform for 1 to over 100 players.",
  "author": "RefinedSoftwareLLC <mikar_gibbros@yahoo.com>",
  "license": "AGPL-3.0+",
  "config": {
    "engine-strict": true,
    "init-license": "AGPL-3.0+"
  },
  "private": true,
  "scripts": {
    "dev": "node source/server.js",
    "start": "node source/server.js",
    "unit": "cross-env jest --coverage",
    "unit:watch": "cross-env jest --watch",
    "e2e": "",
    "test": "npm run unit && npm run e2e",
    "test:watch": "npm run unit:watch",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chingu-voyage7/Bears-Team-16.git"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "node": "^11.2.0",
    "uws": "^10.148.1"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2"
  },
  "engines": {
    "node": ">= 10.3.0",
    "npm": ">= 6.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
